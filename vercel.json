{
  "version": 2,
  "builds": [
    {
      "src": "April/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "April/$1"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "wearenatureent.com"
        }
      ],
      "destination": "https://www.wearenatureent.com/",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "wearenatureent.com"
        }
      ],
      "destination": "https://www.wearenatureent.com/:path*",
      "permanent": true
    }
  ]
}
